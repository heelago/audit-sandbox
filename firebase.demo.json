{
  "apphosting": {
    "backendId": "auditsandbox-demo",
    "rootDir": ".",
    "ignore": [
      "node_modules",
      "node_modules/**",
      ".next",
      ".next/**",
      ".next/cache",
      ".next/cache/**",
      ".next/dev/cache",
      ".next/dev/cache/**",
      ".firebase",
      ".firebase/**",
      ".pnpm-store",
      ".pnpm-store/**",
      ".git",
      ".git/**",
      ".claude",
      ".config",
      "backups",
      "backups/**",
      "tmp_prompt_doc",
      "tmp_prompt_doc/**",
      "mock assignments for demo",
      "mock assignments for demo/**",
      "New folder",
      "New folder/**",
      "prisma/dev.db",
      "prisma/dev.db-journal",
      "*.tsbuildinfo",
      "*.zip",
      "*.docx",
      "*.pdf",
      ".env",
      ".env.*",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions",
      "**/*.ps1",
      "**/*.md"
    ],
    "alwaysDeployFromSource": true
  }
}
